{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ],
    "builds": [
      {
        "src": "api/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "env": {
      "SUPABASE_URL": "@SUPABASE_URL",
      "SUPABASE_KEY": "@SUPABASE_KEY"
    }
  }